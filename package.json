{
  "name": "claude-command",
  "version": "1.0.0",
  "description": "Multi-Claude Command Center - Dashboard for managing parallel Claude AI instances",
  "author": "John",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/claude-command.git"
  },
  "keywords": [
    "claude",
    "ai",
    "automation",
    "dashboard",
    "parallel",
    "mcp",
    "anthropic"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "worker": "tsx src/workers/claude-worker.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "docker:build": "docker-compose -f docker/docker-compose.yml build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.17.0",
    "@trpc/client": "^11.0.0-rc.332",
    "@trpc/next": "^11.0.0-rc.332",
    "@trpc/react-query": "^11.0.0-rc.332",
    "@trpc/server": "^11.0.0-rc.332",
    "bullmq": "^5.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "drizzle-orm": "^0.29.3",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.309.0",
    "next": "^14.1.0",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
